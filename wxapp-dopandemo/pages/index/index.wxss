/**index.wxss**/
swiper{
  height:200px;
}
swiper image{
  width:100%;
  height:200px
}

/*movies item*/
.part{
  width:100%;
  padding-top:10rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap 
    
}
.part-title{
   font-size:30rpx;
}
.item{
  width:32.5%;
  height:300rpx;
  background:#f9f9f9;
  box-sizing: border-box;
  margin-bottom: 10rpx;
  margin-right:1%;

}
.item:nth-of-type(2n){
  margin-right:0;
}
.item-images{
  width:100%;
  height:200rpx;
}
.item-name{
  font-size:30rpx;
  text-indent:10rpx;
  padding-bottom: 15rpx;
}
.item-tag{
  font-size:25rpx;
  color:#999;
  text-indent:10rpx;
}